{
  "name": "seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-jest": "^24.9.0",
    "babel-polyfill": "6.26.0",
    "connected-react-router": "6.4.0",
    "env-cmd": "9.0.3",
    "formik": "1.5.7",
    "history": "4.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "2.9.0",
    "react-redux": "7.1.0",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "^3.0.1",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-saga": "1.0.3",
    "styled-components": "4.3.2",
    "superagent": "5.1.0",
    "typesafe-actions": "^4.4.1"
  },
  "scripts": {
    "start": "REACT_APP_ENV=local react-app-rewired start",
    "build": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test --maxWorkers=4",
    "eject": "react-app-rewired eject",
    "analyze": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build --analyze-bundle",
    "test:coverage": "npm run test -- --coverage",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "lint-style": "stylelint 'src/**/*.style.{ts,tsx}' 'src/**/style.{ts,tsx}'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/*.js",
      "maxSize": "200 kB"
    }
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/enzyme": "^3.9.3",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-intl": "^2.3.18",
    "@types/react-redux": "^7.1.0",
    "@types/react-router": "^5.0.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.16",
    "@types/superagent": "^4.1.1",
    "@types/webpack-env": "^1.13.9",
    "babel-plugin-styled-components": "1.10.1",
    "cypress": "^3.3.1",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.7.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "plop": "2.4.0",
    "prettier": "1.18.2",
    "react-app-rewire-webpack-bundle-analyzer": "1.0.1",
    "react-app-rewired": "2.1.3",
    "react-test-renderer": "16.8.6",
    "redux-saga-test-plan": "4.0.0-beta.3",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2",
    "wait-on": "^3.2.0",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
